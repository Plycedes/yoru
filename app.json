{
    "expo": {
        "name": "yoru",
        "slug": "yoru",
        "scheme": "yoru",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true,
            "package": "com.blacksteel.yoru"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/app-icon.png",
                "backgroundColor": "transparent"
            },
            "package": "com.steel.yoru"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-video",
                {
                    "supportsBackgroundPlayback": true,
                    "supportsPictureInPicture": true
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "cd936f2e-0f14-47ad-b735-6d9fbfeaec0d"
            }
        }
    }
}

